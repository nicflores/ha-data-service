cabal-version:      3.8
name:               hs-data-service
version:            0.1.0.0
license:            NONE
author:             Nic Flores
maintainer:         flores314@gmail.com
build-type:         Simple
extra-doc-files:    CHANGELOG.md
common warnings
    ghc-options: -Wall -O2
executable hs-user-service
    import:           warnings
    main-is:          Main.hs
    other-modules:    Config
    build-depends:    base ^>=4.18.3.0
        , scotty
        , text
        , amazonka
        , amazonka-s3
        , amazonka-sts
        , lens
        , http-conduit
        , http-types
        , bytestring
        , time
        , aeson
    build-tool-depends: patat:patat
    hs-source-dirs:   src, app
    default-language: Haskell2010
